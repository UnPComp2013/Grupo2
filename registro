#include <stdio.h>
#include <stdlib.h>
#include <string.h>

struct cadastro {
    char user[20];
    char pass[20];
    char login[20];
    char senha[20];
} cadastro;

char *cadastro_login()
{
    struct cadastro usuario[1];

    system("cls");
    printf("\n\tCadastro");
    puts("\n\n\tDigite o login: ");
    printf("\t");
    scanf(" %s",usuario[0].user);
    return (usuario[0].user);
}
